{
  "name": "update-node-modules",
  "version": "1.0.5",
  "main": "./dist/main.js",
  "repository": "https://github.com/poad/update-node-modules.git",
  "author": "Kenji Saito <ken-yo@mbr.nifty.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm clean && pnpm lint --fix && tsc --project tsconfig-app.json",
    "lint": "eslint src/**/*.ts src/*.ts tests/*.ts",
    "test": "NODE_ENV=test node --experimental-vm-modules --unhandled-rejections=strict ./node_modules/jest/bin/jest.js --coverage --passWithNoTests --detectOpenHandles",
    "package": "ncc build --source-map --license licenses.txt -m -q",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.73",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.27",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "jest-util": "^29.6.2",
    "regenerator-runtime": "^0.13.11",
    "swc-loader": "^0.2.3",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@actions/core": "^1.10.0"
  },
  "exports": {
    ".": {
      "node": "./dist/main.js",
      "require": "./dist/main.cjs",
      "default": "./dist/main.js"
    }
  },
  "jest": {
    "clearMocks": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "moduleNameMapper": {
      "^@App/(.*)$": "<rootDir>/src/$1"
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "verbose": true
  }
}
